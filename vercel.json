{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/lejcolia/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/lejcolia/favicon.(svg|png|ico)",
      "dest": "/favicon.$1"
    },
    {
      "src": "/lejcolia/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/lejcolia",
      "dest": "/index.html"
    }
  ]
}

