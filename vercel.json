{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/lejcolia/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/lejcolia/favicon.(svg|png|ico)",
      "destination": "/favicon.$1"
    },
    {
      "source": "/lejcolia/(.*)",
      "destination": "/$1"
    },
    {
      "source": "/lejcolia",
      "destination": "/index.html"
    }
  ]
}

