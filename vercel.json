{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/lejcolia/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/lejcolia/favicon.svg",
      "destination": "/favicon.svg"
    },
    {
      "source": "/lejcolia/favicon.png",
      "destination": "/favicon.png"
    },
    {
      "source": "/lejcolia/:path*",
      "destination": "/:path*"
    },
    {
      "source": "/lejcolia",
      "destination": "/index.html"
    }
  ]
}

